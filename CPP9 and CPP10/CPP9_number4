#include <iostream>
#include <string>

int stringLength(const std::string& s) {
    int count = 0;
    for (char c : s) count++;
    return count;
}

int main() {
    std::string text;
    std::cout << "Enter a string: ";
    std::getline(std::cin, text);

    std::cout << "Length: " << stringLength(text) << "\n";
    return 0;
}
